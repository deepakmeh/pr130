<script>
  var waveUtils = (function () {
    async function addToCart(items) {
      var r = await fetch('/cart/add.js', {
        method: 'POST',
        credentials: 'same-origin',
          headers: {
            'Content-Type': 'application/json',
          },
        body: JSON.stringify({items: items})
      });
      return await r.json();
    }

    async function changeCart(key, qty) {
      const body = JSON.stringify({
        quantity: qty,
        id: key
      });
      console.log({
        'body': body
      })
      return await fetch('/cart/change' + '.js', {
        method: 'POST',
        credentials: 'same-origin',
          headers: {
            'Content-Type': 'application/json',
          },
        body: body
      }).then(function(r) {return r.json()});
    }

    async function getCart() {
      var r = await fetch('/cart' + '.js');
      return await r.json();
    };

    function cartToItemQtyMap(cart) {
      const mp = {};
      for (const item of cart.items) {
        mp[item.key] = item.quantity;
      }
      return mp;
    }

    function isDiffCart(cartX, cartY) {
      const mX = cartToItemQtyMap(cartX);
      const mY = cartToItemQtyMap(cartY);
      for (const k in mX) {
        if (mX[k] != mY[k]) {
          return true;
        }
      }
      for (const k in mY) {
        if (mX[k] != mY[k]) {
          return true;
        }
      }
      return false;
    }

    function isCheckingOut() {
      return Shopify.Checkout && (Shopify.Checkout.page === 'show' || Shopify.Checkout.page === 'stock_problems');
    }

    function isNonCheckout() {
      return !Shopify.Checkout;
    }

    function refreshCart() {
      if (isCheckingOut()) {
        window.location.reload();
      } else if (isNonCheckout()) {
        $(`[data-section-type="cart"]`).trigger('refresh');
      }
    }

    function computeItemsMetric(items) {
      var metric = {
        original_total_price: 0,
        total_price: 0,
        quantity: 0
      };
      for (const item of items) {
        metric.original_total_price += item.original_line_price;
        metric.total_price += item.line_price;
        metric.quantity += item.quantity;
      }
      return metric;
    }

    async function cleanUp(cart) {
      let tbd = cart.items.filter(item => '__tbd' in item.properties);
      for (const item of tbd) {
        await changeCart(item.key, 0);
      }

      if (tbd.length > 0) {
        return await waveUtils.getCart();
      }
      return cart;
    }

    function Mutex() {
      let current = Promise.resolve();
      this.lock = () => {
        let _resolve;
        const p = new Promise(resolve => {
          _resolve = () => resolve();
        });
        const rv = current.then(() => _resolve);
        current = p;
        return rv;
      };
    }

    return {
      addToCart: addToCart,
      changeCart: changeCart,
      cleanUp: cleanUp,
      computeItemsMetric: computeItemsMetric,
      getCart: getCart,
      isCheckingOut: isCheckingOut,
      isDiffCart: isDiffCart,
      isNonCheckout: isNonCheckout,
      refreshCart: refreshCart,
      mutex: new Mutex(),
    };
  })();

  var shippingCoverageConfig = {
    
    'variantId': 41761038663879,
    
    'quantity': 1
  }
  function shippingCoverage(config) {
    function isEligible(cart) {
      return cart.attributes["Shipping Insurance"] == 'Yes';
    }

    function isInsuranceItem(item) {
      return item.id == config.variantId;
    }

    async function add(cart) {
      var insuranceItems = cart.items.filter(isInsuranceItem);
      var insuranceItemsMetric = waveUtils.computeItemsMetric(insuranceItems);
      var newQty = config.quantity - insuranceItemsMetric.quantity;
      if (newQty > 0) {
        await waveUtils.addToCart([{
          id: config.variantId,
          quantity: newQty
        }]);
      }
      return newQty;
    }

    async function remove(cart) {
      var insuranceItems = cart.items.filter(isInsuranceItem);
      for (const item of insuranceItems) {
        await waveUtils.changeCart(item.key, 0);
      }
      return insuranceItems.length;
    }

    async function run(cart) {
      if (isEligible(cart)) {
        const newQty = await add(cart);
        return newQty > 0;
      } else {
        const newQty = await remove(cart);
        return newQty > 0;
      }
      return false;
    }

    return {
      run: run
    };
  };


  async function cartAdjustments() {
    const unlock = await waveUtils.mutex.lock();
    try {
      const origCart = await waveUtils.getCart();
      let cart = origCart;
      cart = await waveUtils.cleanUp(cart);

      const updated = await shippingCoverage(shippingCoverageConfig).run(cart);
      if (updated) {
        cart = await waveUtils.getCart();
      }

      cart = await waveUtils.cleanUp(cart);
      if (waveUtils.isDiffCart(origCart, cart)) {
        waveUtils.refreshCart();
      }
      await unlock();
    } catch (error) {
      await unlock();
      console.log(error);
    }
  }

  cartAdjustments();

  document.addEventListener('cart:rerendered', async function(event) {
    await cartAdjustments();
  });

  if (waveUtils.isNonCheckout()) {
    document.addEventListener('cart:shippingCoverage.changed', async function(event) {
      await cartAdjustments();
    });
  }
</script>

